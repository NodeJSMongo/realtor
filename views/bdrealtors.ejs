<% include partials/header %>
<% include partials/loggedin %>
<% include partials/navbar %>

<div class="properties">
  <div class="row">
    <% for(var i = 0; i < users.length; i++ ){ %>
          <div class="card col-sm-4 card_style" style="width: 12rem;">
            <%
            var image ;
            if(users[i] == users[2]){
              image = "https://scontent.fyyz1-1.fna.fbcdn.net/v/t1.0-9/10314592_700130673376598_7054400586872462732_n.jpg?_nc_cat=101&_nc_ht=scontent.fyyz1-1.fna&oh=3e2d5345bb4fa463f9bf5e891fc81c5a&oe=5D4C23FB";
            }else{
              image = "https://scontent.fyyz1-1.fna.fbcdn.net/v/t31.0-8/1237384_581574738565526_531009304_o.jpg?_nc_cat=110&_nc_ht=scontent.fyyz1-1.fna&oh=ed04e4678788bb9d1e3009c430576cae&oe=5D3E3ADE";
            } %>
            <img class="card-img-top realtor_image" src="<%= image %>" alt="Card image cap">
            <div class="card-body">
              <h5 class="card-title"><%= users[i].username %></h5>
              <small class=""> <strong> <%= users[i].city %> | <%= users[i].division %> </strong> </small>
            </div>
            <ul class="list-group list-group-flush">
              <small> <li class="list-group-item"> <%= users[i].brokerage %></small>
            </ul>
            <div class="card-body">
              <a href="#" class="card-link">Contact: <%= users[i].phone %> </a>
            </div>
          </div>
      <% } %>
  </div>
  <div class="properties_error">
    <% if(noMatch !== null) { %>
      <h4><%= noMatch %> </h4>
      <% } %>
  </div>
</div>

<% include partials/footer %>
